{
    "name": "template-repo-ts",
    "version": "1.5.3",
    "repository": {
        "type": "git",
        "url": "https://github.com/ultimateai/template-repo-ts.git"
    },
    "private": true,
    "scripts": {
        "start": "node ./dist/src/server.js",
        "dev": "ts-node-dev src/server.ts",
        "devDebug": "node --inspect=9229 -r ts-node/register src/server.ts",
        "test": "jest --testRegex='./tests/.*.(spec|test).ts$'",
        "lint": "eslint . --ext .ts,.tsx",
        "lintFix": "eslint . --ext .ts,.tsx --fix",
        "artifactregistry-login": "npx google-artifactregistry-auth",
        "prepare": "node ./prepare.js",
        "prebuild": "npm i --no-audit && npm run clean",
        "build": "npm i --no-audit && tsc --skipLibCheck -p tsconfig-build.json",
        "clean": "rimraf ./dist"
    },
    "dependencies": {
        "@automapper/classes": "^8.7.7",
        "@automapper/core": "^8.7.7",
        "@sentry/integrations": "^6.15.0",
        "@sentry/node": "^6.15.0",
        "@sentry/tracing": "^6.15.0",
        "@sentry/types": "^6.15.0",
        "@typegoose/typegoose": "^10.0.0",
        "@ultimate/events-protobuf": "^0.14.0",
        "axios": "^1.3.3",
        "builder-pattern": "^1.3.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cookie-parser": "^1.4.6",
        "dotenv": "^8.2.0",
        "envalid": "^6.0.1",
        "express": "^4.17.1",
        "express-prom-bundle": "^6.4.1",
        "helmet": "^3.21.3",
        "hpp": "^0.2.3",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "mongoose": "^6.8.2",
        "nyc": "^15.1.0",
        "prom-client": "^14.0.1",
        "redis": "^4.5.1",
        "reflect-metadata": "^0.1.13",
        "retry-axios": "^3.0.0",
        "swagger-ui-express": "^4.2.0",
        "tslog": "^3.3.2",
        "url-value-parser": "^2.0.3",
        "uuid": "^8.3.2",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.3",
        "@types/helmet": "0.0.45",
        "@types/hpp": "^0.2.2",
        "@types/ioredis": "^4.28.1",
        "@types/jest": "^29.4.0",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/lodash": "^4.14.177",
        "@types/morgan": "^1.9.0",
        "@types/node": "^14.17.34",
        "@types/redis": "^2.8.32",
        "@types/supertest": "^2.0.8",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.4",
        "@types/yamljs": "^0.2.31",
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/eslint-plugin-tslint": "^5.47.1",
        "@typescript-eslint/parser": "^5.47.1",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.2",
        "jest": "^29.4.2",
        "lint-staged": "^13.1.0",
        "mongodb-memory-server": "^8.11.4",
        "nock": "^13.2.1",
        "prettier": "^2.2.1",
        "rimraf": "^4.1.2",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.5.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.4"
    },
    "engines": {
        "npm": ">=9.0.0 <10.0.0",
        "node": ">=18.0.0 <19.0.0"
    }
}